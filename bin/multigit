#!/usr/bin/env node
var multigit = require('../');

try {	
    var m = process.argv[2].split('.');	
    if(m.length > 0)
        multigit.commands[m[0]][m[1]]();
    else
        multigit.commands[m[0]]();
    return;
}
catch(err) {
  console.log('[Error: The command ' + process.argv[2] + ' does not exist.]');
  return;
}